{
    "name": "aurora/http-transaction",
    "description": "HTTP Transaction manage a request to a response workflow",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "psr/http-message": "~1.0",
        "panlatent/context": " ~1.0",
        "aurora/http-foundation": "~1.0",
        "aurora/http-message-filter": "~1.0",
        "http-interop/http-server-middleware": "^1.0",
        "http-interop/http-server-handler": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "panlatent",
            "email": "panlatent@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Aurora\\Http\\Transaction\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "suggests": {
        "guzzlehttp/psr7": "PSR-7 HTTP message library for guzzle",
        "aurorahttp/http-message": "boost http message for aurorahttp"
    }
}
